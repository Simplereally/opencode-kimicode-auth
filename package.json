    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Simplereally/opencode-kimicode-auth.git"
    },
    "homepage": "https://github.com/Simplereally/opencode-kimicode-auth#readme",
    "bugs": {
        "url": "https://github.com/Simplereally/opencode-kimicode-auth/issues"
